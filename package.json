        "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^27.5.0",
        "babel-loader": "^8.2.3",
        "babel-plugin-module-resolver": "^4.1.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
