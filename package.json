        "eslint-plugin-react": "^7.28.0",
        "husky": "^7.0.4",
        "jest": "^27.5.0",
        "lodash-webpack-plugin": "^0.11.6",
        "node-sass": "^7.0.1",
        "prettier": "^2.5.1",
        "prettier-eslint": "^13.0.0",
        "react": "^17.0.2",
